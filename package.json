{
  "name": "configg",
  "description": "A node configuration manager",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "test": "test",
    "doc": "doc"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "cson": "^3.0.2",
    "debug": "^2.2.0",
    "hashtree": "^0.7.0",
    "hjson": "^1.7.6",
    "js-yaml": "^3.5.3",
    "json5": "^0.4.0",
    "lodash": "^4.4.0",
    "properties": "^1.2.1",
    "toml": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^2.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  },
  "scripts": {
    "test": "npm run lint; mocha test/*.mocha.js",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks test/*.mocha.js",
    "lint": "eslint --quiet --cache '**/*.js'",
    "doc": "jsdox -o docs index.js lib/*.js",
    "clean": "rm -rf jsdoc coverage"
  },
  "repository": {
    "type": "git",
    "url": "github.com/commenthol/configg"
  },
  "keywords": [
    "conf",
    "config",
    "configuration",
    "node-config",
    "config-node",
    "env",
    "environment"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/commenthol/configg/issues"
  },
  "homepage": "https://github.com/commenthol/configg",
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  }
}
